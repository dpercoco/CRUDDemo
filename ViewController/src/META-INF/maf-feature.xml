<?xml version="1.0" encoding="UTF-8" ?>
<adfmf:features xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:adfmf="http://xmlns.oracle.com/adf/mf">
<adfmf:feature id="crud.About" name="About">
    <adfmf:content id="crud.About.1">
      <adfmf:amx file="crud.About/about.amx"/>
    </adfmf:content>
    <adfmf:properties id="p3">
      <adfmf:property name="iOSWebView" value="default" id="p4"/>
    </adfmf:properties>
  </adfmf:feature>
  <adfmf:feature id="crud.CRUD" name="CRUD">
    <adfmf:content id="crud.CRUD.2">
      <adfmf:constraints>
        <adfmf:constraint property="hardware.screen.diagonalSize" operator="less" value="7" id="c1"/>
      </adfmf:constraints>
      <adfmf:amx file="crud.CRUD/phone/taskflow.xml#taskflow"/>
    </adfmf:content>
    <adfmf:content id="crud.CRUD.1">
      <adfmf:amx file="crud.CRUD/taskflow.xml#taskflow"/>
    </adfmf:content>
    <adfmf:properties id="p5">
      <adfmf:property name="iOSWebView" value="default" id="p6"/>
    </adfmf:properties>
  </adfmf:feature>
  <adfmf:feature id="crud.Springboard" name="Springboard">
    <adfmf:content id="crud.Springboard.1">
      <adfmf:amx file="crud.Springboard/springboard.amx"/>
    </adfmf:content>
    <adfmf:properties id="p1">
      <adfmf:property name="iOSWebView" value="default" id="p2"/>
    </adfmf:properties>
  </adfmf:feature>
  <adfmf:feature id="crud.Recipes" name="Recipes" image="images/forkSpoon.png">
    <adfmf:content id="crud.Recipes.1">
      <adfmf:amx file="crud.Recipes/Recipes-task-flow.xml#Recipes-task-flow">
        <adfmf:includes>
          <adfmf:include type="JavaScript" file="js/displayUrl.js" id="i3"/>
          <adfmf:include type="StyleSheet" file="css/main.css" id="i4"/>
        </adfmf:includes>
      </adfmf:amx>
    </adfmf:content>
  </adfmf:feature>
  <adfmf:feature id="crud.Aisles" name="Aisles" image="images/storefront.png">
    <adfmf:content id="crud.Aisles.1">
      <adfmf:amx file="crud.Aisles/Aisles-task-flow.xml#Aisles-task-flow">
        <adfmf:includes>
          <adfmf:include type="StyleSheet" file="css/main.css" id="i5"/>
          <adfmf:include type="JavaScript" file="js/expandcollapse.js" id="i6"/>
          <adfmf:include type="StyleSheet" file="css/expandcollapse.css" id="i7"/>
        </adfmf:includes>
      </adfmf:amx>
    </adfmf:content>
  </adfmf:feature>
  <adfmf:feature id="crud.Ingredients" name="Ingredients">
    <adfmf:content id="crud.Ingredients.1">
      <adfmf:amx file="crud.Recipes/EditIngredients.amx">
        <adfmf:includes>
          <adfmf:include type="JavaScript" file="js/expandcollapse.js" id="i1"/>
          <adfmf:include type="StyleSheet" file="css/expandcollapse.css" id="i2"/>
        </adfmf:includes>
      </adfmf:amx>
    </adfmf:content>
  </adfmf:feature>
</adfmf:features>
