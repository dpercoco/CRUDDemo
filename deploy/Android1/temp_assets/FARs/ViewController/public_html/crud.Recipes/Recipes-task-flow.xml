<?xml version="1.0" encoding="UTF-8" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="Recipes-task-flow">
    <default-activity>Recipes</default-activity>
    <managed-bean id="__5">
      <managed-bean-name>IngredientsBean</managed-bean-name>
      <managed-bean-class>crud.JAVA.IngredientsBean</managed-bean-class>
      <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <view id="Recipes">
      <page>/crud.Recipes/ListRecipe.amx</page>
    </view>
    <task-flow-call id="Aisles">
      <task-flow-reference>
        <document>/crud.Aisles/Aisles-task-flow.xml</document>
        <id>AislesTaskFlow</id>
      </task-flow-reference>
    </task-flow-call>
    <view id="DetailRecipe">
      <page>/crud.Recipes/DetailRecipe.amx</page>
    </view>
    <method-call id="clearParms">
      <method>#{bindings.clearParms.execute}</method>
      <outcome>
        <fixed-outcome>edit</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Create">
      <method>#{bindings.Create.execute}</method>
      <outcome>
        <fixed-outcome>Create</fixed-outcome>
      </outcome>
    </method-call>
    <view id="EditRecipe">
      <page>/crud.Recipes/EditRecipe.amx</page>
    </view>
    <view id="Google">
      <page>/crud.Recipes/Google.amx</page>
    </view>
    <view id="EditIngredient">
      <page>/crud.Recipes/EditIngredient.amx</page>
    </view>
    <!--<method-call id="findAll">
    </method-call>-->
    <control-flow-rule id="__1">
      <from-activity-id>Recipes</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>aisles</from-outcome>
        <to-activity-id>Aisles</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__4">
        <from-outcome>add</from-outcome>
        <to-activity-id>Create</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__24">
        <from-outcome>detail</from-outcome>
        <to-activity-id>DetailRecipe</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>DetailRecipe</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>clear</from-outcome>
        <to-activity-id>clearParms</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__9">
        <from-outcome>editRecipe</from-outcome>
        <to-activity-id>EditRecipe</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>clearParms</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>edit</from-outcome>
        <to-activity-id>EditRecipe</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>Create</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>Create</from-outcome>
        <to-activity-id>EditRecipe</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__14">
      <from-activity-id>EditRecipe</from-activity-id>
      <control-flow-case id="__15">
        <from-outcome>google</from-outcome>
        <to-activity-id>Google</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__2">
        <from-outcome>editIngredient</from-outcome>
        <to-activity-id>EditIngredient</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__19">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__20">
        <from-outcome>goFindAll</from-outcome>
        <to-activity-id>findAll</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__21">
      <from-activity-id>findAll</from-activity-id>
      <control-flow-case id="__22">
        <from-outcome>goEditRecipes</from-outcome>
        <to-activity-id>EditRecipe</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
