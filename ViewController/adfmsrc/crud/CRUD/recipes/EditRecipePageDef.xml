<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.2.1.22.33" id="EditRecipePageDef"
                Package="crud.CRUD.recipes">
  <parameters/>
  <executables>
    <iterator Binds="root" RangeSize="25" DataControl="RecipeService" id="RecipeServiceIterator"/>
    <accessorIterator MasterBinding="RecipeServiceIterator" Binds="recipes" RangeSize="25" DataControl="RecipeService"
                      BeanClass="crud.JAVA.Recipe" id="recipesIterator">
      <sortCriteria>
        <sort attribute="title" ascending="true"/>
      </sortCriteria>
    </accessorIterator>
    <accessorIterator MasterBinding="recipesIterator" Binds="ingredients" RangeSize="25" DataControl="RecipeService"
                      BeanClass="crud.JAVA.Ingredient" id="ingredientsIterator"/>
  </executables>
  <bindings>
    <attributeValues IterBinding="recipesIterator" id="title">
      <AttrNames>
        <Item Value="title"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="recipesIterator" id="recipeUrl">
      <AttrNames>
        <Item Value="recipeUrl"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="recipesIterator" id="photo">
      <AttrNames>
        <Item Value="photo"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="recipesIterator" id="description">
      <AttrNames>
        <Item Value="description"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="recipesIterator" id="prepTime">
      <AttrNames>
        <Item Value="prepTime"/>
      </AttrNames>
    </attributeValues>
    <action IterBinding="recipesIterator" id="removeRow" InstanceName="crudmodel.RecipeService.recipes"
            DataControl="RecipeService" RequiresUpdateModel="false" Action="removeRowWithKey">
      <NamedData NDName="rowKey" NDType="java.lang.String"/>
    </action>
    <methodAction IterBinding="recipesIterator" id="reloadFromStore" RequiresUpdateModel="true" Action="invokeMethod"
                  MethodName="reloadFromStore" IsViewObjectMethod="false" DataControl="RecipeService"
                  InstanceName="bindings.recipesIterator.currentRow.dataProvider" IsLocalObjectReference="true"/>
    <methodAction IterBinding="recipesIterator" id="saveToStore" RequiresUpdateModel="true" Action="invokeMethod"
                  MethodName="saveToStore" IsViewObjectMethod="false" DataControl="RecipeService"
                  InstanceName="bindings.recipesIterator.currentRow.dataProvider" IsLocalObjectReference="true"/>
    <attributeValues IterBinding="recipesIterator" id="id">
      <AttrNames>
        <Item Value="id"/>
      </AttrNames>
    </attributeValues>
    <methodAction id="reloadRecipes" RequiresUpdateModel="true" Action="invokeMethod" MethodName="reloadRecipes"
                  IsViewObjectMethod="false" DataControl="RecipeService"
                  InstanceName="data.RecipeService.dataProvider"/>
    <attributeValues IterBinding="RecipeServiceIterator" id="googleCriteria">
      <AttrNames>
        <Item Value="googleCriteria"/>
      </AttrNames>
    </attributeValues>
    <methodAction id="callGoogle" RequiresUpdateModel="true" Action="invokeMethod" MethodName="callGoogle"
                  IsViewObjectMethod="false" DataControl="RecipeService"
                  InstanceName="data.RecipeService.dataProvider"/>
    <methodAction id="deleteRecipe" RequiresUpdateModel="true" Action="invokeMethod" MethodName="deleteRecipe"
                  IsViewObjectMethod="false" DataControl="RecipeService" InstanceName="data.RecipeService.dataProvider">
      <NamedData NDName="id" NDValue="#{bindings.id.inputValue}" NDType="java.lang.Integer"/>
    </methodAction>
    <attributeValues IterBinding="RecipeServiceIterator" id="selectedTitle">
      <AttrNames>
        <Item Value="selectedTitle"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="RecipeServiceIterator" id="selectedUrl">
      <AttrNames>
        <Item Value="selectedUrl"/>
      </AttrNames>
    </attributeValues>
    <methodAction IterBinding="recipesIterator" id="deleteFromStore" RequiresUpdateModel="true" Action="invokeMethod"
                  MethodName="deleteFromStore" IsViewObjectMethod="false" DataControl="RecipeService"
                  InstanceName="bindings.recipesIterator.currentRow.dataProvider" IsLocalObjectReference="true">
      <NamedData NDName="id" NDValue="#{bindings.id.inputValue}" NDType="java.lang.Integer"/>
    </methodAction>
    <tree IterBinding="ingredientsIterator" id="ingredients">
      <nodeDefinition DefName="crud.JAVA.Ingredient" Name="ingredients0">
        <AttrNames>
          <Item Value="toString"/>
          <Item Value="id"/>
          <Item Value="item"/>
          <Item Value="foodGroup"/>
          <Item Value="newItem"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <methodAction IterBinding="ingredientsIterator" id="addIngredientToStore" RequiresUpdateModel="true"
                  Action="invokeMethod" MethodName="addIngredientToStore" IsViewObjectMethod="false"
                  DataControl="RecipeService" InstanceName="bindings.ingredientsIterator.currentRow.dataProvider"
                  IsLocalObjectReference="true"/>
    <methodAction IterBinding="ingredientsIterator" id="deleteIngredientFromStore" RequiresUpdateModel="true"
                  Action="invokeMethod" MethodName="deleteIngredientFromStore" IsViewObjectMethod="false"
                  DataControl="RecipeService" InstanceName="bindings.ingredientsIterator.currentRow.dataProvider"
                  IsLocalObjectReference="true"/>
    <action IterBinding="ingredientsIterator" id="Create" RequiresUpdateModel="true" Action="createRow"/>
    <methodAction IterBinding="recipesIterator" id="reloadIngredients" RequiresUpdateModel="true" Action="invokeMethod"
                  MethodName="reloadIngredients" IsViewObjectMethod="false" DataControl="RecipeService"
                  InstanceName="bindings.recipesIterator.currentRow.dataProvider" IsLocalObjectReference="true"/>
    <methodAction IterBinding="recipesIterator" id="refreshIngredients" RequiresUpdateModel="true" Action="invokeMethod"
                  MethodName="refreshIngredients" IsViewObjectMethod="false" DataControl="RecipeService"
                  InstanceName="bindings.recipesIterator.currentRow.dataProvider" IsLocalObjectReference="true"/>
  </bindings>
</pageDefinition>
